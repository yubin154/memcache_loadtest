<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
  <version>2</version>
  <threadsafe>true</threadsafe>
  <env>flex</env>
  <service>memcache-test</service>
  <resources>
    <cpu>32</cpu>
    <memory-gb>64</memory-gb>
    <disk-size-gb>100</disk-size-gb>
  </resources>
  <automatic-scaling>
    <min-num-instances>1</min-num-instances>
    <max-num-instances>1</max-num-instances>
  </automatic-scaling>
  <beta-settings>
    <setting name="source_reference" value="https://github.com/yubin154/java-docs-samples.git#03821c13c85cc319578407935f36920b03ea6c3e"/>
    <setting name="enable_app_engine_apis" value="True"/>
  </beta-settings>
</appengine-web-app>
